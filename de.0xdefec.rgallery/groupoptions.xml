<?xml version="1.0"?>
<!DOCTYPE data SYSTEM "http://www.woltlab.com/DTDs/options.dtd">
<data>
	<import>
		<categories>
			<category name="user.rgallery">
				<parent>user</parent>
			</category>
		</categories>
		<options>
			<option name="user.rgallery.canView">
				<categoryname>user.rgallery</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<enableoptions>user.rgallery.canUpload</enableoptions>
			</option>
			<option name="user.rgallery.canUpload">
				<categoryname>user.rgallery</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<hidden>1</hidden>
				<enableoptions>user.rgallery.keepOriginal,user.rgallery.quota_disabled,user.rgallery.uploadsPerWeek_disabled,user.rgallery.quota,user.rgallery.uploadsPerWeek</enableoptions>
			</option>
			<option name="user.rgallery.keepOriginal">
				<categoryname>user.rgallery</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<hidden>1</hidden>
			</option>
			<option name="user.rgallery.quota_disabled">
				<categoryname>user.rgallery</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<hidden>1</hidden>
			</option>
			<option name="user.rgallery.quota">
				<categoryname>user.rgallery</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>0</defaultvalue>
				<hidden>1</hidden>
			</option>
			<option name="user.rgallery.uploadsPerWeek_disabled">
				<categoryname>user.rgallery</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<hidden>1</hidden>
			</option>
			<option name="user.rgallery.uploadsPerWeek">
				<categoryname>user.rgallery</categoryname>
				<optiontype>integer</optiontype>
				<defaultvalue>0</defaultvalue>
				<hidden>1</hidden>
			</option>
		</options>
		<categories>
			<category name="mod.rgallery">
				<parent>mod</parent>
			</category>
		</categories>
		<options>
			<option name="mod.rgallery.canModerate">
				<categoryname>mod.rgallery</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
			</option>
		</options>
	</import>
</data>
